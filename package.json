	},
	"type": "module",
	"dependencies": {
		"axios": "^1.7.4",
		"clsx": "^2.1.1",
		"layerchart": "^0.44.0",
		"mode-watcher": "^0.4.1",
		"svelte-legos": "^0.2.4",
		"svelte-sonner": "^0.3.27",
		"tailwind-merge": "^2.5.0",
		"tailwind-variants": "^0.2.1",
		"vaul-svelte": "^0.3.2"
	}
}
